{
  "name": "@kibisis/pinakion-contract",
  "description": "The Kibisis Pinakion NFT contract written in TEALScript.",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kibis-is/pinakion-token"
  },
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "algorand",
    "avm",
    "blockchain",
    "voi"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "compile": "npm run clean && tealscript src/contracts/Pinakion.avm.ts dist",
    "clean": "shx rm -rf dist && shx mkdir dist",
    "generate:client": "algokit generate client dist/ --language typescript --output .client/index.ts",
    "lint": "eslint . --ext .ts",
    "reset:localnet": "algokit localnet reset",
    "start:localnet": "algokit localnet start",
    "stop:localnet": "algokit localnet stop",
    "test": "./scripts/test.sh"
  },
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^2.3.1",
    "@algorandfoundation/tealscript": "latest",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "algosdk": "^2.9.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "5.0.2",
    "uuid": "^11.0.2"
  }
}
